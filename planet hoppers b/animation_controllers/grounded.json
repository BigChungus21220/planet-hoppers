{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.grounded": {
			"states": {
				"default": {
					"transitions": [
						{
							"on_ground": "q.is_on_ground"
						}
					],
					"on_entry": [
						"/tag @s remove grounded"
					]
				},
				"on_ground": {
					"transitions": [
						{
							"default": "!q.is_on_ground"
						}
					],
					"on_entry": [
						"/tag @s add grounded"
					]
				}
			}
		}
	}
}