{
  "format_version": "1.16.100",
  "minecraft:block": {
    "description": {
        "identifier": "ek:oxygen_plant_empty"
    },
    "components": {
      "minecraft:loot": "loot_tables/blocks/oxygen_plant.json",
      "minecraft:geometry": "geometry.cross_plant",
      "minecraft:pick_collision": {
        "origin": [-3, 0, -3],
        "size": [6, 10, 6]
      },
      "minecraft:material_instances": {
        "*": {
          "texture": "oxygen_plant_empty",
          "render_method": "alpha_test"
        }
      },
      "minecraft:block_light_absorption" : 0,
      "minecraft:block_light_emission" : 0,
      "minecraft:placement_filter": {
        "conditions": [
          {
            "allowed_faces": ["up"]
          }
        ]
      },
      "minecraft:random_ticking": {
        "on_tick": {
          "event": "grow",
          "target": "self"
        }
      },
      "minecraft:entity_collision": false
    },
    "events": {
      "grow": {
        "run_command": {
          "command": [
            "setblock ~ ~ ~ ek:oxygen_plant"
          ]
        }
      }
    }
  }
}